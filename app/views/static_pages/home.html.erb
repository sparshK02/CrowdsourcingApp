<% provide(:title, "Home") %>
<div class="center jumbotron">
    <h1>Welcome to the Annotation Tool</h1>
    <br>
    <% if logged_in? %>
           <p><%= link_to "View Task Here", current_user, class: "btn btn-lg btn-primary"%></p>
        <% else %>
